{
  "extends": "@react-native/typescript-config",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@app/*": [
        "./src/app/*"
      ],
      "@features/*": [
        "./src/features/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ],
      "@routing/*": [
        "./src/routing/*"
      ],
      "@shared/*": [
        "./src/shared/*"
      ],
      "@widgets/*": [
        "./src/widgets/*"
      ]
    }
  }
}
